<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Login</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="css\styles.css">

        

</head>
<body>
  <div class="form-container">
    <form  method="POST" action="/student_login" class="stu-form teacher-login container-fluid" style="width: 50%;">
        <h1 class="mb-4" style="text-align: center; font-weight: 800;">STUDENT LOGIN</h1>
        <!-- Email input -->
        <div class="form-outline mb-4">
          <input type="email" name="studentEmail" placeholder="Email Address" id="form2Example1" class="form-control" />
          
        </div>
      
        <!-- Password input -->
        <div class="form-outline mb-4">
          <input type="password" name="studentPassword"  placeholder="Password" id="form2Example2" class="form-control" />
          
        </div>
    
      
        <!-- Submit button -->
        <button type="submit" class="btn btn-success btn-block mb-4">Sign in</button>
      
        <!-- Register buttons -->
        <div class="text-center">
          <p>New to QUIZZOO? <a href="/student_register">Register</a></p>
         
        </div>
      </form>
    </div>
    <%- include('footer'); -%>
</body>
</html>